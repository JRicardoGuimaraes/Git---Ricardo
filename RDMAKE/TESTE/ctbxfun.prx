							oPrint:Say(nLin,nCol,cImpSaldo,oFont08)
						Endif					
					Else
						@ nLin, nCol pSay nSaldo Picture cPicture
					Endif
				EndIf		
			EndIf		
	    Else	//Se nao utiliza conceito de conta estourada
			If cTipo == "1" 								// Conta Devedora
				If cTipoSinal == "S"              			// Sinal
