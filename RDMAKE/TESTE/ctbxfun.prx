		dbUseArea(.T.,"TOPCONN",TcGenQry(,,cQuery),"SLDTOTCTI",.T.,.F.)		
		
		TcSetField("SLDTOTCTI","SLDANTDB"  ,"N",__aTamVlr[1],__aTamVlr[2])	
